    <script>
        (function ($) {
            $.fn.pop = function () {
                var top = this.get(-1);
                this.splice(this.length - 1, 1);
                return top;
            };

            $.fn.shift = function () {
                var bottom = this.get(0);
                this.splice(0, 1);
                return bottom;
            };
        })(jQuery);

        function JobickCarousel() {

            /*  testimonial one function by = owl.carousel.js */
            jQuery('.front-view-slider').owlCarousel({
                loop: false,
                margin: 30,
                nav: true,
                autoplaySpeed: 3000,
                navSpeed: 3000,
                autoWidth: true,
                paginationSpeed: 3000,
                slideSpeed: 3000,
                smartSpeed: 3000,
                autoplay: false,
                animateOut: 'fadeOut',
                dots: true,
                navText: ['', ''],
                responsive: {
                    0: {
                        items: 1
                    },

                    480: {
                        items: 1
                    },

                    767: {
                        items: 3
                    },
                    1750: {
                        items: 3
                    }
                }
            })
        }

        jQuery(window).on('load', function () {
            setTimeout(function () {
                JobickCarousel();
            }, 1000);
        });

        //#region DeleteAllCheckboxSelected
        $("#single-select").change(function () {
            unCheckedCheckbox();
        });
        function unCheckedCheckbox() {
            const checkboxs = document.querySelectorAll(".form-check-input");
            checkboxs.forEach(checkbox => {
                checkbox.checked = false;
            });
        }
        //#endregion

        //#region LoadingPage
        $(window).on("load", function (event) {
            const statusButtons = document.querySelectorAll(".form-check-input");
            statusButtons.forEach(checkbox => {
                checkbox.value == 1 ? checkbox.checked = true : checkbox.checked = false
            });
        });
        //#endregion

        //#region LoadingAnimation
        function createPreloaderMethod() {
            $(".loading").append('<div id="preloader"><div class="lds-ripple"><div></div><div></div></div></div>');
        }
        function deletePreloaderMethod() {
            jQuery('#preloader').remove();
            $('#main-wrapper').addClass('show');
        }
        //#endregion

        //#region Ajax
        function ajaxMethod(url, data, type, dataType) {
            createPreloaderMethod();
            var response
            $.ajax({
                url: url,
                async: false,
                data: data,
                type: type,
                dataType: dataType,
                success: function (result) {
                    response = result
                },
                error: function (result) {
                    response = result
                },
            });
            deletePreloaderMethod();
            return response;
        }
        //#endregion

        //#region IsActive
        async function isActiveMethod(that, content, dataStatus, url) {
            var response = null;
            await Swal.fire({
                title: 'İşlemi onaylıyor musunuz?',
                text: content,
                icon: 'question',
                showCancelButton: true,
                confirmButtonColor: '#DD6B55',
                cancelButtonColor: '#aaa',
                confirmButtonText: 'Evet',
                cancelButtonText: 'Hayır'
            }).then((result) => {
                if (result.isConfirmed) {
                    $(that).is(':checked') == true ? dataStatus.status = 1 : dataStatus.status = 0;
                    response = ajaxMethod(url, dataStatus, "POST", "Json");
                }
            });
            return response;
            that.toArray().pop();
        }
        //#endregion

        //#region Delete
        async function deleteMethod(that, content, dataDelete, url) {
            var response;
            await Swal.fire({
                title: 'İşlemi onaylıyor musunuz?',
                text: content,
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#DD6B55',
                cancelButtonColor: '#aaa',
                confirmButtonText: 'Onayla',
                cancelButtonText: 'Vazgeç'
            }).then((result) => {
                if (result.isConfirmed) {
                    response = ajaxMethod(url, dataDelete, "POST", "Json");
                }
                else if (result.isDismissed) {
                    response.isSuccess = false;
                }
            });
            return response;
            that.toArray().pop();
        }
        //#endregion

        //#region SuccessMessage
        function successMessageFunc(result) {
            let toastrOptions = {
                'location': "top-end",
                'type': "",
                'content': "",
            };
            if (result?.isSuccess == true) {
                toastrOptions.type = "success";
                toastrOptions.content = result.statusMessage;
                alertBox(toastrOptions);
                return true;
            }
            else if (result?.isSuccess == false || result == null) {
                toastrOptions.type = "error";
                toastrOptions.content = result != null ? result.statusMessage : "Bir hata oluştu.";
                alertBox(toastrOptions);
                return false;
            }
            else {
                toastrOptions.type = "error";
                toastrOptions.content = "Bir hata oluştu.";
                alertBox(toastrOptions);
                return false;
            }
        }
        //#endregion

        //#region ErrorMessage
        function errorMessageFunc(result) {
            var toastrOptions = {
                'location': "top-end",
                'type': "",
                'content': "",
            };
            toastrOptions.type = "error";
            toastrOptions.content = result?.statusMessage ? result.statusMessage : "Bir hata ile karşılaşıldı";
            alertBox(toastrOptions);
            return false;
        }
        //#endregion

        //#region InfoModel
        function infoModal(content) {
            Swal.fire({
                title: 'Hatırlatma',
                text: content,
                icon: 'info',
                confirmButtonColor: '#3085d6',
                confirmButtonText: 'Tamam'
            });
        }
        //#endregion

        //#region TransactionModals
        async function transactionModal(content) {
            return Swal.fire({
                title: 'İşlemi Onaylıyor musunuz?',
                text: content,
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#DD6B55',
                cancelButtonColor: '#aaa',
                confirmButtonText: 'Evet Onaylıyorum',
                cancelButtonText: 'Hayır Onaylamıyorum',
            },
                function (isConfirm) {
                    if (isConfirm) {
                        return true;
                    } else {
                        return false;
                    }
                });
        }

        async function transactionModalInput(content) {
            const { value: text } = await Swal.fire({
                input: 'textarea',
                input: 'text',
                inputLabel: 'Lütfen Bir Açıklama Girin',
                inputAttributes: {
                    'aria-label': 'Type your message here'
                },
                showCancelButton: true
            })

            if (text) {
                var result = {
                    'message': text,
                    'success': true
                }
              return result;
            }else{
                 var result = {
                    'message': "naber lan",
                    'success': false
                }
              return result;
            }

        }
        //#endregion

        //#region AlertBox
        function alertBox(toastrOptions) {
            Swal.fire({
                position: toastrOptions.location != "" ? toastrOptions.location : 'top-end',
                icon: toastrOptions.type,
                title: toastrOptions.content != "" ? toastrOptions.content : "İşlem Başarılı.",
                showConfirmButton: false,
                timer: toastrOptions.timer != undefined || null ? toastrOptions.timer : 1500
            });
        }
        //#endregion

        //#region UploadImage
        function uploadImage(file) {
            return new Promise((resolve, reject) => {
                const fileReader = new FileReader();
                fileReader.readAsDataURL(file);

                fileReader.onload = () => {
                    if ((file.size / 1024 / 1024) <= 5) {
                        let request = { fileName: file.name, data: fileReader.result }
                        resolve(request);
                    }
                    else {
                        let toastrOptions = {
                            'location': "top-end",
                            'type': "error",
                            'content': "Dosya boyutu 5 Mb'dan büyük olamaz",
                        };
                        alertBox(toastrOptions);
                    }
                };

                fileReader.onerror = (error) => {
                    reject(error);
                };
            });
        }
        //#endregion

    </script>


